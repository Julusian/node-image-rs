{
  "name": "@julusian/image-rs",
  "version": "2.0.2",
  "description": "Limited image-rs bindings for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "napi": {
    "binaryName": "julusian-image-rs",
    "targets": [
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "aarch64-pc-windows-msvc",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "armv7-unknown-linux-gnueabihf"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Julusian/node-image-rs"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.5.0",
    "@types/node": "^20.19.27",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.18"
  },
  "scripts": {
    "build": "napi build --platform --release --js-package-name @image-rs/target --no-js --dts bindings.d.ts --no-const-enum",
    "build:debug": "napi build --platform",
    "test": "vitest"
  },
  "files": [
    "prebuilds",
    "*.js",
    "*.d.ts"
  ],
  "packageManager": "yarn@4.12.0"
}
