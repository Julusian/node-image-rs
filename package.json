{
  "name": "@julusian/image-rs",
  "version": "0.2.1",
  "description": "Limited image-rs bindings for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "julusian-image-rs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/julusian/node-image-rs.git"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/node": "^20.11.24",
    "ava": "^6.1.2",
    "prettier": "^3.2.5"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "build": "napi build --platform --release --js-package-name @image-rs/target --js=false --dts bindings.d.ts",
    "build:debug": "napi build --platform",
    "test": "ava"
  },
  "packageManager": "yarn@4.1.1"
}
