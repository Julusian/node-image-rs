{
  "name": "@julusian/image-rs",
  "version": "1.0.3",
  "description": "Limited image-rs bindings for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "julusian-image-rs",
    "triples": {
      "additional": [
        "aarch64-pc-windows-msvc",
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Julusian/node-image-rs"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/node": "^20.14.9",
    "ava": "^6.1.3",
    "prettier": "^3.3.2"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "build": "napi build --platform --release --js-package-name @image-rs/target --js=false --dts bindings.d.ts",
    "build:debug": "napi build --platform",
    "test": "ava"
  },
  "packageManager": "yarn@4.3.1"
}
